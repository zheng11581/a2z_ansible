server{
    listen 80;
    listen 443 ssl;
    server_name {{domain_name}};
    root {{nginx_www}};
    index index.html index.htm index.jsp;
    ssl on;
    ssl_certificate   {{nginx_home}}/conf.d/{{ssl_cert}};
    ssl_certificate_key  {{nginx_home}}/conf.d/{{ssl_key}};
    ssl_session_timeout 5m;
    ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE:ECDH:AES:HIGH:!NULL:!aNULL:!MD5:!ADH:!RC4;
    ssl_protocols TLSv1 TLSv1.1 TLSv1.2;
    ssl_prefer_server_ciphers on;
    error_page  404 403 500 502 503 504  /404.html;
    client_max_body_size 10M;
    location = /doctor/getCode {
        if ( $request_method = GET ){
            return 404;
        }
        proxy_pass http://localhost:8185;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /recordfile {
        proxy_pass http://43.254.25.186;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        add_header Access-Control-Allow-Origin *;
        add_header Access-Control-Allow-Headers "Origin, X-Requested-With, Content-Type, Accept";
        add_header Access-Control-Allow-Methods "GET, POST, OPTIONS";
    }
    location / {
        proxy_pass http://localhost:8888;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /doctor {
        proxy_pass http://localhost:8185;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /JobAdmin {
        proxy_pass http://localhost:8190;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /websocket5 {
        proxy_pass http://localhost:8184/chat/websocket5;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /websocket {
        proxy_pass http://localhost:8184/chat/websocket;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_read_timeout 120s;
        proxy_send_timeout 120s;
        proxy_connect_timeout 120s;
    }
    location /websocket4 {
    	proxy_pass http://localhost:8184/chat/websocket4;
    	proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    	proxy_set_header X-Forwarded-Proto $scheme;
   		proxy_set_header X-Forwarded-Port $server_port;
    	proxy_set_header Upgrade $http_upgrade;
    	proxy_set_header Connection "upgrade";
    	proxy_read_timeout 120s;
    	proxy_send_timeout 120s;
    	proxy_connect_timeout 120s;
    }
    location /file/mdt {
        expires 30d;
        root /glzt/dataFile/images;
        add_header    Cache-Control  max-age=86400;
        add_header Access-Control-Allow-Origin *;
        add_header Access-Control-Allow-Methods 'GET, POST, OPTIONS';
        add_header Access-Control-Allow-Headers 'DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Authorization,lang,access-token';
    }
    location /vwlabrf/getVwLabRf {
        proxy_pass http://43.254.25.189:8081;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP   $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
        proxy_set_header X-Forwarded-Port $server_port;
        proxy_redirect off;
        client_max_body_size 20M;
    }
    location /followup {
        try_files $uri $uri/ /followup/index.html;
    }
    location ~* \.(gif|jpg|jpeg|ico|txt|png|webp|svg)$ {
	    expires 30d;
        root /glzt/dataFile/images;
	add_header    Cache-Control  max-age=86400;
      	add_header Access-Control-Allow-Origin *;
    	add_header Access-Control-Allow-Methods 'GET, POST, OPTIONS';
    	add_header Access-Control-Allow-Headers 'DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Authorization,lang,access-token';
    }
    location /file/invoice { 
        expires 30d;
        root /glzt/dataFile/images;
        add_header    Cache-Control  max-age=86400;
        add_header Access-Control-Allow-Origin *;
        add_header Access-Control-Allow-Methods 'GET, POST, OPTIONS';
        add_header Access-Control-Allow-Headers 'DNT,X-Mx-ReqToken,Keep-Alive,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Authorization,lang,access-token';
    }
    location ~* \.(mp3)$ {
    	expires 30d;
    	root /glzt/dataFile/audio;
    	add_header    Cache-Control  max-age=86400;
    }
    location /jump.html {
        rewrite /var/www/html/jump.html last;
    }
    location /jumpUrl.html {
        rewrite /var/www/html/jumpUrl.html last;
    }
    location /dataGather {
    	proxy_pass http://43.254.25.186:8081;
    	proxy_set_header Host $host;
    	proxy_set_header X-Real-IP   $remote_addr;
    	proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    	proxy_set_header X-Forwarded-Proto $scheme;
    	proxy_set_header X-Forwarded-Port $server_port;
    	proxy_redirect http:// $scheme://;
    }
}
